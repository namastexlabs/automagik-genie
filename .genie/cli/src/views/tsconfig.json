{
  "compilerOptions": {
    "module": "ES2020",
    "moduleResolution": "node",
    "outDir": "../../.temp-esm-views",
    "rootDir": ".",
    "target": "ES2020",
    "strict": false,
    "noEmitOnError": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "declaration": false,
    "jsx": "react",
    "jsxFactory": "React.createElement",
    "jsxFragmentFactory": "React.Fragment"
  },
  "include": ["init-wizard.tsx"]
}
