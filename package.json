  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "bin/",
    "lib/",
    "templates/",
    "README.md",
    ".claude/"
  ],
  "dependencies": {
    "axios": "^1.11.0",
    "colors": "^1.4.0",
    "fs-extra": "^11.3.0",
    "inquirer": "^8.2.6",
    "semver": "^7.7.2",
